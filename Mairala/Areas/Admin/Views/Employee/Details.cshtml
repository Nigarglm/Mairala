
@model List<Employee>

@foreach (Employee employees in Model)
{
    <div class="card" style="width: 18rem;">
      <img src="~/assets/images/@employees.ImageUrl" class="card-img-top" alt="...">
      <div class="card-body">
        <p class="card-text">@employees.Name @employees.Surname</p>
        <p class="card-text">@employees.Position.Name</p>
      </div>
    </div>
}
