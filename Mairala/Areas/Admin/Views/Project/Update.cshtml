@model UpdateProjectVM

<div class="row">
    <div class="col-md-6 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Create</h4>
                <form class="forms-sample" method="post" enctype="multipart/form-data">
                    <div asp-validation-summary="ModelOnly"></div>
                    <div class="form-group">
                        <label asp-for="Photo">Photo</label>
                        <input asp-for="Photo" type="text" class="form-control" id="exampleInputUsername1" placeholder="Photo">
                        <span asp-validation-for="Photo"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Title">Title</label>
                        <input asp-for="Title" type="text" class="form-control" id="exampleInputUsername1" placeholder="Title">
                        <span asp-validation-for="Title"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Description">Description</label>
                        <input asp-for="Description" type="text" class="form-control" id="exampleInputUsername1" placeholder="Description">
                        <span asp-validation-for="Description"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="CategoryId">Category Id</label>
                        <input asp-for="CategoryId" type="text" class="form-control" id="exampleInputUsername1" placeholder="CategoryId">
                        <span asp-validation-for="CategoryId"></span>
                    </div>
                    <button type="submit" class="btn btn-primary me-2">Create</button>
                    <button asp-action="index" asp-controller="home" class="btn btn-light">Cancel</button>
                </form>
            </div>
        </div>
    </div>
</div>
